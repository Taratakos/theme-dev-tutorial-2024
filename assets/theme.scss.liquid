/*================ Rich Text Editor ================*/
.rte {
  *{
    margin: 0;
  }

  >*:not(:last-child) {
    margin-bottom: spacing()/2;

    @include media-query($large-up) {
      margin-bottom: spacing();
    }
  }
  
  img {
    height: auto;
  }

  table {
    table-layout: fixed;
  }

  ul,
  ol {
    margin: 0 0 (spacing() / 2) spacing();
  }

  // Match the styles from RTE nested lists
  ul {
    list-style: disc outside;

    ul {
      list-style: circle outside;

      ul {
        list-style: square outside;
      }
    }
  }
}

.text-center.rte,
.text-center .rte {
  ul,
  ol {
    margin-left: 0;
    list-style-position: inside;
  }
}

// allow table to scroll for tables in the RTE since we don't know
// how many columns they will contain. Class added by JS.
// sass-lint:disable no-misspelled-properties
.rte-table {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

// This class is wrapped around YouTube/Vimeo embeds in the RTE
// to make them responsive and prevent layout breaking
.rte__video-wrapper {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  padding-bottom: 56.25%;
  height: 0;
  height: auto;

  iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}

// This class is wrapped around tables in the RTE
// to prevent layout breaking with a scrollable parent
.rte__table-wrapper {
  max-width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

/*============================================================================
  Responsive tables, defined with .responsive-table on table element.
==============================================================================*/
@include media-query($small) {
  .responsive-table {
    width: 100%;

    thead {
      display: none;
    }

    tr {
      display: block;
    }

    // IE9 table layout fixes
    tr,
    td {
      float: left;
      clear: both;
      width: 100%;
    }

    th,
    td {
      display: block;
      text-align: right;
      padding: spacing() / 2;
      margin: 0;
    }

    td::before {
      content: attr(data-label);
      float: left;
      text-align: center;
      padding-right: 10px;
    }
  }

  // Add a keyline between rows
  .responsive-table-row + .responsive-table-row,
  tfoot > .responsive-table-row:first-child {
    position: relative;
    margin-top: 10px;
    padding-top: spacing() / 2;

    &::after {
      content: '';
      display: block;
      position: absolute;
      top: 0;
      left: spacing() / 2;
      right: spacing() / 2;
      border-bottom: 1px solid $color-border;
    }
  }
}

/*================ Links & Buttons ================*/
.btn,
.shopify-challenge__container .shopify-challenge__button {
  @include prefix('user-select', 'none', moz ms webkit spec);
  @include prefix(appearance, none, webkit moz spec);
  display: inline-block;
  width: auto;
  min-width: rem(325px);
  text-decoration: none;
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
  border: rem(2px) solid $black;
  border-radius: 0;
  padding: rem(20px);
  font-family: font-family(avenir);
  font-weight: 900;
  font-size: rem(15px);
  position: relative;
  color: $black;
  background-color: transparent;
  transition: background-color 0.2s ease-in-out;
  text-transform: uppercase;

  &:after{
    content: attr(data-text);
    position: absolute;
    top: -0.125rem;
    left: -0.125rem;
    width: calc(100% + 0.25rem);
    height: calc(100% + 0.25rem);
    border: rem(2px) solid $black;
    background-color: $offgold;
    padding: rem(20px);
    transition: transform 0.2s ease-in-out, background-color 0s ease-in-out;
    transform-origin: center;
    transition-delay: 0s, 0.2s;
    color: $black;
  }

  &:before{
    content: '';
    position: absolute;
    top: -0.125rem;
    left: -0.125rem;
    width: calc(100% + 0.25rem);
    height: calc(100% + 0.25rem);
    border: rem(2px) solid $black;
    background-color: $red;
    padding: rem(20px);
    transition: transform 0.2s ease-in-out;
    transform-origin: center;
  }

  &:active{
    background-color: $offgold;

    &:after{
      background-color: $blue;
      transform: translate(rem(10px), rem(-10px));
      transition: transform 0.2s ease-in-out, background-color 0s ease-in-out;
      transition-delay: 0s, 0s;
      color: $white;
    }

    &:before{
      transform: translate(rem(5px), rem(-5px));
    }
  }

  &:focus{
    background-color: $offgold;

    &:after{
      background-color: $blue;
      transform: translate(rem(10px), rem(-10px));
      transition: transform 0.2s ease-in-out, background-color 0s ease-in-out;
      transition-delay: 0s, 0s;
      color: $white;
    }

    &:before{
      transform: translate(rem(5px), rem(-5px));
    }
  }

  @include media-query($large-up) {
    &:hover{
      background-color: $offgold;

      &:after{
        background-color: $blue;
        transform: translate(rem(10px), rem(-10px));
        transition: transform 0.2s ease-in-out, background-color 0s ease-in-out;
        transition-delay: 0s, 0s;
        color: $white;
      }

      &:before{
        transform: translate(rem(5px), rem(-5px));
      }
    }
  }
}

.btn--reverse {
  color: $black;
  background-color: transparent;

  &:after{
    background-color: $blue;
    color: $white;
  }

  &:before{
    background-color: $red;
  }

  &:active{
    background-color: $blue;

    &:after{
      background-color: $offgold;
      color: $black;
    }
  }

  &:focus{
    background-color: $blue;

    &:after{
      background-color: $offgold;
      color: $black;
    }
  }

  @include media-query($large-up) {
    &:hover{
      background-color: $blue;

      &:after{
        background-color: $offgold;
        color: $black;
      }
    }
  }
}

.btn--simple {
  display: inline-block;
  width: auto;
  text-decoration: none;
  text-align: center;
  text-transform: uppercase;
  vertical-align: middle;
  white-space: nowrap;
  border-radius: 0;
  padding: rem(20px);
  font-family: font-family(avenir);
  font-weight: 900;
  font-size: rem(15px);
  letter-spacing: 0.07142em;
  position: relative;
  color: $black;
  transition: background-color 0.2s ease-in-out;

  &.btn--offgold {
    border: rem(2px) solid $black;
    background-color: $offgold;

    &:focus{
      background-color: $blue;
      color: $white;
    }

    @include media-query($large-up) {
      &:hover{
        background-color: $blue;
        color: $white;
      }
    }
  }

  &.btn--link {
    border: 0;
    background-color: transparent;

    &:focus{
      text-decoration: underline;
    }

    @include media-query($large-up) {
      &:hover{
        text-decoration: underline;
      }
    }
  }
}
/*============================================================================
  Form scaffolding
    - Selectors setup for you to style form elements how you want
    - Focus, error, and disabled states are set to be extended
==============================================================================*/
input,
textarea,
select {
  border: 1px solid $color-border;
  border-radius: 0;
  max-width: 100%;
  font-family: font-family(avenir);
  font-weight: 500;

  &:focus {
    // You should add some focus styles
  }

  &[disabled] {
    cursor: default;
    background-color: $color-disabled;
    border-color: $color-disabled-border;
  }
}

textarea {
  min-height: 100px;
}

/*================ Custom select style ================*/
select {
  @include prefix(appearance, none, webkit moz spec);
  background-position: right center;
  background: {
    image: url($svg-select-icon);
    repeat: no-repeat;
    position: right 10px center;
    color: transparent;
    size: rem(12px);
  }
  padding-right: 28px;
  text-indent: 0.01px;
  text-overflow: '';
  cursor: pointer;

  /*================ Hide the svg arrow in IE9 ================*/
  .ie9 & {
    padding-right: 10px;
    background-image: none;
  }
}

select::-ms-expand {
  display: none;
}

/*================ Error styles ================*/
input,
select,
textarea {
  &.input-error {
    border-color: $color-error;
    background-color: $color-error-bg;
    color: $color-error;
  }
}

.errors ul {
  list-style-type: none;
  color: $color-error;
  padding: 0;
  margin-top: 0;
  font-family: font-family(avenir);
  font-size: rem(14px);
  line-height: 1.64;
  letter-spacing: 0.07142em;
}

$color-blankstate: rgba($color-body-text, 0.35);
$color-blankstate-border: rgba($color-body-text, 0.2);
$color-blankstate-background: rgba($color-body-text, 0.1);

.placeholder-svg {
  display: block;
  fill: $color-blankstate;
  background-color: $color-blankstate-background;
  width: 100%;
  height: 100%;
  max-width: 100%;
  max-height: 100%;
  border: 1px solid $color-blankstate-border;
}

.placeholder-svg--small {
  width: 480px;
}

.placeholder-noblocks {
  padding: 40px;
  text-align: center;
}

// Mimic a background image by wrapping the placeholder svg with this class
.placeholder-background {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;

  .icon {
    border: 0;
  }
}

/*================ Drawers ================*/
.is-transitioning {
  // sass-lint:disable no-important
  display: block !important;
  visibility: visible !important;
}

.drawer {
  // sass-lint:disable no-misspelled-properties
  display: none;
  position: fixed;
  top: 0;
  bottom: 0;
  overflow: hidden;
  -webkit-overflow-scrolling: touch;
  z-index: $z-index-drawer;
  background-color: #fff;
  transition: all 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);
  width: 100%;
  max-width: rem(300px);
}

.js-drawer-open {
  overflow: hidden;
}

.drawer--top {
  width: 100%;
  transform: translateY(0);

  .js-drawer-open-top & {
    transform: translateY(100%);
    display: block;
  }
}

.drawer--left {
  left: 0;
  transform: translateX(-100%);

  .js-drawer-open-left & {
    display: block;
    transform: translateX(0);

    .lt-ie9 & {
      left: 0;
    }
  }
}

.drawer--right {
  right: 0;
  transform: translateX(100%);

  .js-drawer-open-right & {
    display: block;
    transform: translateX(0);

    .lt-ie9 & {
      right: 0;
    }
  }
}

.drawer-page-content::after {
  visibility: hidden;
  opacity: 0;
  content: '';
  display: block;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: $z-index-drawer - 1;
  transition: all 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);

  .js-drawer-open & {
    visibility: visible;
    opacity: 1;
  }
}


