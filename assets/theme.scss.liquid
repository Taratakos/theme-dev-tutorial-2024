  .arrow-outline{
    fill: $offgold;
  }

  &.product-carousel__arrow-prev{
    left: spacing()/2;
  }

  &.product-carousel__arrow-next{
    right: spacing()/2;
  }

  &:focus{
    .arrow-background{
