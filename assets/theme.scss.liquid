  @media screen and (orientation: landscape) {
    width: 45%;
  }

  @include media-query($medium-up) {
    width: 45%;
  }

  @include media-query($large-up) {
    width: 25%;
  }
}
.product-reviews__title{
  font-family: font-family(avenir);
  font-size: rem(32px);
  font-weight: 900;
  color: $black;
  line-height: 1;
  margin: 0;
  text-align: center;

  @include media-query($large-up) {
