.movement__title{
  width: 100%;
  padding: 0 spacing();
  margin: 0;
  position: relative;
  z-index: 1;
  font-family: font-family(sunflower);
  font-weight: normal;
  @include display-flexbox();
  @include flex-wrap(wrap);
  @include justify-content(center);
  font-size: 13vw;
  line-height: 1;

  @media screen and (min-width: 64rem) {
    font-size: rem(82px);
    @include flex-wrap(nowrap);
  }

  @include media-query($large-up) {
    padding: 0 spacing()*2;
  }
}

.movement-title__word{
  @include display-flexbox();
  @include justify-content(center);
}

.movement-title__word:first-child{
  width: 100%;

  @media screen and (min-width: 64rem) {
    width: auto;
  }
}

.movement-title__letter{
  animation: movement-wave 2s calc(0.1s * var(--a)) ease-in-out infinite;
  transform: translateY(25%);


  @media screen and (min-width: 64rem) {
    animation-duration: 2.5s;
    animation-delay: calc(0.1s * var(--i));
  }
}

@keyframes movement-wave{
  0% {
    transform: translateY(25%);
  }
  50%{
    transform: translateY(-25%);
  }
}

.movement-title__space{
  width: 4vw;
}

.movement__content{
  width: 100%;
  z-index: 1;
  position: relative;
  white-space: nowrap;
  font-size: 0;
  padding: spacing() 0;

  @include media-query($large-up) {
    padding: spacing() spacing()/2*3;
  }
}

.movement__card{
  width: 80%;
  padding: 0 spacing();
  display: inline-block;
  margin: spacing()*2 0 spacing();

  @include media-query($medium-up) {
    width: 45%;
  }

  @media screen and (min-width: 64rem){
    width: 33.333%;
  }
}

.movement-card__link{
  display: block;
  position: relative;

  &:before{
    content: '';
    width: 100%;
    height: 100%;
    border: rem(2px) solid $black;
    background-color: $white;
    position: absolute;
    top: spacing()/2;
    left: spacing()/2 * -1;
    z-index: 0;
  }

  &:focus{
    &:before{
      background-color: $offgold;
    }
  }

  @include media-query($large-up) {
    &:hover{
      &:before{
        background-color: $offgold;
      }
    }
  }
}

.movement-card__image{
  width: 100%;
  position: relative;
  z-index: 3;
}

.movement-card__text{
  width: 100%;
  text-align: center;
  z-index: 2;
  position: absolute;
  top: 0;
  left: 0;
  padding: spacing();

  @include media-query($medium-up) {
    padding: spacing()*3 spacing();
  }

  @media screen and (min-width: 64rem){
    padding: spacing();
  }
}

.movement-card__kicker{
  font-family: font-family(gotham);
  font-weight: bold;
  font-size: rem(15px);
  margin: 0 0 spacing()/2;
}

.movement-card__title{
  font-family: font-family(sunflower);
  font-size: rem(36px);
  font-weight: normal;
  line-height: 1;
  margin: 0;
  white-space: normal;
}

.movement-card__background{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border: rem(2px) solid $black;
  z-index: 1;
}
.homepage-product{
  position: relative;
  overflow: hidden;

  &:before{
    content: '';
    background-color: white;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: rem(200px);

    @include media-query($medium-up) {
      display: none;
      content: none;
    }
  }

  #rc_radio_options{
    border: rem(2px) solid $white;
    border-bottom-width: 0;
  }

  div#rc_container{
    margin-bottom: 0;
  }

  div.rc_block__type{
    border-radius: 0;
    background-color: transparent;
    padding: spacing()/2;
    position: relative;

    &:not(:first-child) {
      border-top: rem(2px) solid $white;
    }

    @include media-query($large-up) {
      padding: spacing();
    }
  }

  input.rc_radio{
    @include visually-hidden();
  }

  label.rc_label{
    font-family: font-family(avenir);
    font-size: rem(15px);
    font-weight: 900;
    text-transform: uppercase;
    color: $white;
    position: relative;
    padding-left: spacing()*2;
  }

  label.rc_label__deliver_every{
    padding-top: spacing();
  }

  span.rc_label__message{
    background-color: $offgold;
    padding: rem(5px) rem(10px);
    border-radius: rem(4px);
  }

  span.rc_price{
    color: $offgold;
    font-size: rem(18px);
    line-height: 1;
  }

  div.rc_block__type>label.rc_label{
    &:before{
      content: '';
      position: absolute;
      top: 50%;
      left: 0;
      transform: translateY(-50%);
      height: rem(15px);
      width: rem(15px);
      border-radius: 50%;
      background-color: transparent;
      border: rem(1px) solid $white;
    }
  }

  div.rc_block__type--active>label.rc_label{
    &:after{
      content: '';
      position: absolute;
      top: 50%;
      left: rem(3px);
      transform: translateY(-50%);
      height: rem(9px);
      width: rem(9px);
      border-radius: 50%;
      background-color: $offgold;
    }
  }

  div.rc_block__type--active label.rc_label{
    color: $white;
  }

  select.rc_select{
    border: rem(2px) solid $white;
    padding: rem(10px) rem(30px) rem(10px) rem(10px);
    font-weight: 900;
    color: $white;
    width: rem(130px);

    &:focus{
      border-color: $offgold;
    }
  }

  div.rc_popup{
    display: none;
  }

  

  div.rc_popup__hover{
    @include display-flexbox();
    @include align-items(center);
    font-family: font-family(avenir);
    font-weight: 500;
    font-size: rem(12px);
    color: $white;

    &:focus{
      color: $offgold;
    }

    &.focus-within{
      color: $offgold;
    }
  }
}

.homepage-product__container{
  max-width: rem(1024px);
  margin: 0 auto;

  @include media-query($medium-up) {
    display: grid;
    padding: spacing();
    grid-template-columns: repeat(2, 1fr);
    grid-column-gap: spacing();
    padding: spacing();
  }

  @include media-query($large-up) {
    padding: spacing() spacing()*2;
    grid-column-gap: spacing()*2;
  }
}

.homepage-product__carousel{
  position: relative;
  z-index: 1;
  @include display-flexbox();
  @include flex-direction(column);

  &:before{
    content: '';
    background-color: $white;
    width: 500vw;
    height: 500%;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    z-index: 0;
  }

  @include media-query($medium-up) {
    grid-row: 1/span 2;

    &:before{
      content: none;
    }
  }
}

.homepage-product__images-container{
  position: relative;
  @include display-flexbox();
  @include flex-direction(column);
  @include align-items(center);
  @include justify-content(center);
  align-self: start;
  width: 100%;
}

.homepage-product__images{
  width: 100%;
  z-index: 1;
}

.homepage-product__image{
  width: 100%;
}

.homepage-product-image__dots{
  @include display-flexbox();
  @include align-items(center);
  @include justify-content(center);
  list-style-type: none;
  padding: 0;
  margin: spacing()/2 0;
  font-size: 0;
  position: relative;
  z-index: 1;

  @include media-query($large-up) {
    display: none;
  }
}

.homepage-product-image__arrow{
  display: none;
  background-color: transparent;
  padding: rem(5px);
  font-size: 0;
  border: 0;
  top: 50%;
  transform: translateY(-50%);
  position: absolute;
  z-index: 1;

  svg{
    width: rem(35px);
    height: auto;
  }

  .arrow-background{
    fill: $white;
  }

  .arrow-outline{
    fill: $offgold;
  }

  &.homepage-product-image__arrow-prev{
    left: spacing()/2;
  }

  &.homepage-product-image__arrow-next{
    right: spacing()/2;
  }

  &:focus{
    .arrow-background{
      fill: $offgold;
    }

    .arrow-outline{
      fill: $black;
    }
  }

  @include media-query($medium-up) {
    display: block;
  }

  @include media-query($large-up) {
    &:hover{
      .arrow-background{
        fill: $offgold;
      }

      .arrow-outline{
        fill: $black;
      }
    }
  }
}

.homepage-product-image__trigger{
  padding: 0;
  border: 0;
  border-radius: 50%;
  width: rem(5px);
  height: rem(5px);
  background-color: $gray;
  margin: 0 rem(5px);

  .selected &{
    background-color: $offgold;
  }

  &:focus{
    background-color: $red;
  }

  @include media-query($large-up) {
    &:hover{
      background-color: $red;
    }
  }
}

.homepage-product__header{
