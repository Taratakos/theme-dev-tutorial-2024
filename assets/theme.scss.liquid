/*================ TEMPLATES ================*/
/*================ Account Login ================*/

.login {
  @include display-flexbox();
  @include align-items(center);
  font-size: rem(14px);
  padding: 0 spacing() spacing();

  @include media-query($tablet) {
    min-height: 70vh;
    padding: spacing() spacing(extra-loose);
  }
}

.login__container {
  @include display-flexbox();
  @include flex-wrap(wrap);
  @include justify-content(space-between);
  max-width: rem($grid-desktop);
  margin: 0 auto;
}

.login__new,
.login__current,
.login__recover {
  @include media-query($tablet) {
    min-height: rem(371px);
  }
}

.login__new,
.login__current,
.login__create{
  width: 100%;
  margin: 0 auto;
  label {
    display: none;
  }
  
  @include media-query($tablet) {
    width: 50%;
    @include display-flexbox();
    @include flex-direction(column);
    &:first-child {
      padding-right: spacing(loose);
    }
    &:last-child {
      padding-left: spacing(loose)
    }
  }
}

.reset-password-success,
.login__recover {
  display: none;
}

.login__subheader,
.create__subheader {
  font-family: font-family(avenir);
  font-size: rem(16px);
  letter-spacing: 0.2em;
  text-transform: uppercase;
  border-bottom: solid rem(2px) $black;
  margin: 0;
  padding: spacing(loose) 0;
}

.login__content {
  margin: spacing(loose) 0 spacing(extra-loose);
  font-family: font-family(avenir);
  font-size: rem(14px);
  line-height: 1.64;
  letter-spacing: 0.07142em;
  color: #000000;

  @include media-query($tablet) {
    margin: spacing(loose) 0 rem(32px);
  }
}

/*----------- Form and Inputs -----------*/

.login__form {
  @include display-flexbox();
  @include flex-wrap(wrap);
  margin: 0;
  margin-top: spacing(loose);
}

.login-form__recover-container {
  width: 100%;
  text-align: right;
  padding: spacing() 0;
}

.login-form__back-container {
  width: 100%;
  text-align: left;
  padding: spacing() 0;
}

.login-form__cancel-container {
  width: 100%;
  text-align: center;
  padding: spacing() 0;

  button {
    text-transform: uppercase;
    line-height: 0;
  }
}

.login-form__input {
  font-size: rem(14px);
  width: 100%;
  margin-bottom: spacing(loose);
}

.login-form__input:not([type=submit]) {
  padding: 0 spacing();
  height: rem(46px);
  border: solid rem(2px);
  font-weight: font-weight(bold);

  @include placeholder {
    font-weight: font-weight(bold);
    text-transform: uppercase;
    letter-spacing: 0.07142em;
    color: $black;
  }
}

.login-form__input--create {
  @include display-flexbox();
  @include align-items(center);
  @include justify-content(center);
  padding: rem(14px) rem(30px);
  height: rem(55px);
  margin-top: auto;
  font-size: rem(14px);
  width: 100%;
  margin-bottom: spacing(loose);

  @include media-query($tablet) {
    margin-bottom: rem(56px);
  }
}

.login-form__input--sign-in,
.login-form__input--submit {
  height: rem(55px);
  margin-bottom: 0;
}

.login-form__input--recover,
.login-form__input--return,
.login-form__input--cancel {
  font-family: font-family(avenir);
  font-weight: bold;
  text-align: right;
  text-decoration: none;
  letter-spacing: 0.03357em;
  color: $black;

  &:focus {
    text-decoration: underline;
  }

  @include media-query($tablet) {
    &:hover {
      text-decoration: underline;
    }
  }
}

.login-form__input--cancel {
  font-weight: font-weight(extra-bold);
  outline: none;
  background-color: transparent;
  border-color: transparent;
}
/*================ Account ================*/

.account {
  min-height: 70vh;
  font-size: rem(14px);
  padding: spacing();

  @include media-query($tablet) {
    padding: spacing() rem(50px);
  }
  @include media-query($large-up) {
    padding: spacing() rem(70px);
  }
}

.account__container,
.account__main {
  max-width: $width-site;
  margin: 0 auto;
  font-family: font-family(avenir);
}

.account__menu {
  text-align: center;
  padding: 0;
  padding-bottom: spacing(extra-loose);

  @include media-query($tablet) {
    @include display-flexbox();
    @include flex-direction(column);
    text-align: left;
  }
}

.account__menu-container {
  margin-bottom: spacing();

  @include media-query($tablet) {
    margin-bottom: spacing(loose);
  }
}

/*----------- Account Links and Buttons -----------*/

.account__logout,
.account__edit,
.account__delete {
  width: 100%;
  height: auto;
  margin: spacing(extra-tight) 0;
  padding: rem(15px) 0;
  letter-spacing: 0.07142em;

  @include media-query($tablet) {
    width: rem(151px);
  }
}

.account__create,
.account__update {
  width: 100%;
  height: auto;
  margin-top: spacing(loose);
  padding: rem(15px) 0;
  letter-spacing: 0.07142em;
}

.account__return,
.account__cancel,
.account__view-address,
.address__cancel {
  font-family: font-family(avenir);
  color: $black;
  font-weight: font-weight(semi-bold);
  text-decoration: none;
  letter-spacing: 0.07142em;
  background-color: transparent;
  border: 0;
  padding: 0;

  &:focus {
    text-decoration: underline;
  }

  @include media-query($tablet) {
    &:hover {
      text-decoration: underline;
    }
  }
}

.account__logout {
  margin: 0;
}

.account__return {
  font-family: font-family(avenir);
  text-transform: uppercase;
}

.account__view-address {
  text-transform: uppercase;
}

.account__cancel {
  text-transform: uppercase;
  margin-bottom: spacing(extra-tight);
  background-color: transparent;
  border-color: transparent;
  margin: 0;
  padding: 0;
}

/*----------- Account Accordion -----------*/

.account__accordion {
  padding: spacing() 0 spacing(loose);
  width: 100%; 

  @include media-query($tablet) {
    @include display-flexbox();
    @include flex-direction(row-reverse);
    @include justify-content(center);
  }
  @include media-query($desktop) {
    @include justify-content(flex-end);
  }
}

.account__accordion-single {
  border-top: rem(2px) solid $black;
  &:first-child { 
    @include media-query($tablet) {
      padding-left: spacing(extra-loose);
      border: 0;
      @include flex(1);
    }
  }
  &:last-child { 
    border-bottom: rem(2px) solid $black;
    @include media-query($tablet) {
      padding-right: spacing(extra-loose);
      border: 0;
    }
  }
}

.account__accordion-header,
.account__subheader,
.account__section-header {
  font-family: font-family(avenir);
  color: $blue;
  font-size: rem(24px);
  font-weight: font-weight(semi-bold);
  text-transform: lowercase;
  text-align: left;
  padding: spacing() 0;

  @include media-query($large-up) {
    font-size: rem(32px);
  }
}

.account__accordion-header {
  width: 100%;
  background-color: transparent;
  border: 0;

  &:focus { 
    color: $offgold; 
  }
  
  @include media-query($tablet) { 
    display: none; 
  }
}

.account__accordion-header[aria-expanded="false"] {
  color: $black;
}

.account__subheader {
  display: none;

  @include media-query($tablet) { 
    display: block; 
    line-height: 1;
    margin: 0;
  }
}

.account__section-header {
  margin: 0;
  padding: 0;
  line-height: 1;
  margin-bottom: spacing();
}

.account__accordion-header-content {
  &:focus  { 
    outline: none; 
  }
}

.account__accordion-content {
  padding-bottom: spacing();
  p {
    font-size: rem(14px);
    line-height: 1.64;
    letter-spacing: 0.03357em;
    margin: 0;
    margin-bottom: spacing();
  }
  p.account__name {
    margin-bottom: spacing(loose);
  }
}

.account__accordion-content[aria-hidden="true"] {
  display: none;
}

/*----------- Account Table -----------*/

.account__table-container {
  overflow-x: scroll;
}

.account__table {
  width: 100%;
  text-align: left;
  border: 0;
  border-collapse: collapse;

  a {
    color: $black;
    text-decoration: underline;

    &:focus {
      color: $offgold;
      text-decoration: none;
    }
    @include media-query($tablet) {
      &:hover {
        color: $offgold;
        text-decoration: none;
      }
    }
  }

  th {
    text-transform: uppercase;
  }

  th,
  td {
    padding: spacing(loose)/2 0;
    padding-right: spacing(extra-tight);
    line-height: 1.64;
    letter-spacing: 0.03357em;
    border: 0;
    min-width: rem(110px);

    &:last-child {
      padding-right: 0;
    }
  }

  @include media-query($tablet) {
    border-top: solid rem(2px) $black;
    th,
    td {
      border-bottom: solid rem(2px) $black;
    }
  }
}
/*================ Account Order ================*/

.account__order {

  .account__subheader {
    display: block;
    padding: 0;

    &:first-child {
      margin-top: 0;
    }
  }

  .account__section-header--default {
    margin-bottom: spacing(extra-tight);
  }

  @include media-query($tablet) {
    @include display-flexbox();

    .account__subheader {
      display: block;
      padding: spacing() 0;
    }
  }
}

.account-order__overview {
  @include media-query($tablet) {
      width: auto;
      border: 0;
      padding-right: spacing(extra-loose);
      margin-bottom: spacing();
    }

    p {
      font-family: font-family(avenir);
      font-size: rem(14px);
      letter-spacing: 0.03357em;
    }
}

.account-order__details {
  @include media-query($tablet) {
    @include flex(1);
    border: 0;
    padding-left: spacing(extra-loose);
    margin-bottom: spacing();
  }
}

.account-order__date {
  margin: 0;
  margin-bottom: spacing();
  font-family: font-family(avenir);
  font-size: rem(14px);
  letter-spacing: 0.03357em;
}

.account-order__status {
  margin-top: 0;
  margin-bottom: spacing();

  @include media-query($tablet) {
    margin-bottom: spacing(loose);
  }
}

.order-table__line-item-details {
  padding: 0;
  list-style: none;
  margin: 0;
  margin-top: spacing(extra-tight);

  li {
    line-height: 1.64;
    letter-spacing: .47px;
  }
}

.order__table {
  width: 100%;
  border-spacing: 0;
  line-height: 1;
  border-collapse: collapse;
  margin-bottom: spacing();

  @include media-query($tablet) {
    margin-bottom: 0;
  }

  tbody tr {
    border: none;
    border-top: solid rem(2px) $black;

    &:last-of-type {
      border-bottom: solid rem(2px) $black;
    }
    &:nth-child(2n - 1) {
      background-color: #f0f0f0;
    }
  }

  tfoot tr {
    border: none;

    td {
      padding: spacing(super-tight) spacing(extra-tight) 0;
      line-height: 1.64;
    }
  }

}

.order__table thead th {
  font-family: font-family(avenir);
  font-size: rem(14px);
  line-height: 1.64;
  letter-spacing: 0.07142em;
  text-align: right;
  text-align: left;
  text-transform: uppercase;
  padding: 0 spacing(super-tight) spacing(extra-tight);

  &.order-table__money{
    text-align: right;
  }

  &.order-table__qty{
    text-align: center;
  }

  &:first-of-type {
    padding: 0 0 spacing(extra-tight);
  }
}

.order__table td {
  font-size: 14px;
  letter-spacing: 0.03357em;
  padding: spacing(extra-tight) spacing(super-tight);
  

  &.order-table__money {
    text-align: right;
  }

  &.order-table__qty {
    text-align: center;
  }

  &:first-of-type {
    padding-left: spacing(extra-tight);
  }
}

.order__table a {
  background: none;
  border: none;
  padding: 0;
  color: $black;
  margin-left: auto;
  text-decoration: underline;
  text-transform: uppercase;

  &:focus {
    color: $offgold;
    opacity: 0.65;
  }

  @include media-query($large-up) {
    &:hover {
      color: $offgold;
      opacity: 0.65;
    }
  }
}
/*================ Account Address ================*/

.account__addresses {
  label {
    display: none;
  }
  .account-address__country label {
    display: block;
    font-weight: bold;
    letter-spacing: 0.07142em;
    text-transform: uppercase;
    margin-bottom: spacing(super-tight);
  }
  .account__create {
    margin-bottom: spacing();
  }
  form {
    @include display-flexbox();
    @include flex-wrap(wrap);
  }
}

.account-address__default {
  padding-bottom: spacing(extra-tight);
}

.account__section-header--default {
  font-family: font-family(avenir);
  font-size: rem(14px);
  letter-spacing: 0.07142em;
  text-transform: uppercase;
  color: $black;
}

/*----------- Account Additional Address -----------*/

.account-address__addresses {
  @include display-flexbox();
  @include flex-wrap(wrap);
  list-style-type: none;
  padding: 0;
  margin: 0;
}

.account-address__address {
  width: 100%;
  @include media-query($small-up) {
    width: 50%;
  }
}

.account-address__listing {
  padding-bottom: spacing(super-tight);
}

/*----------- Account Address Accordion -----------*/

.account-address__accordion {
  padding: spacing() 0;
  @include media-query($tablet) {
    @include display-flexbox();
    @include justify-content(sspace-between);
  }
  .account__accordion-content {
    padding: 0;
  }
  .account__accordion-content p {
    margin: 0;
  }
}

.account-address__accordion-single {
  border-top: solid rem(2px) $black;
  &:last-child {
    border-bottom: solid rem(2px) $black;
  }

  @include media-query($tablet) {
    width: 50%;
    &:first-child { 
      padding-right: spacing(extra-tight);
      border: 0;
    }
    &:last-child {
      padding-left: spacing(extra-tight); 
      border: 0;
    }
  }
}

/*----------- Account Address Forms and Inputs -----------*/

#account-address-new,
.account-address__edit {
  select,
  input:not([type="submit"]) {
    width: 100%;
    font-size: rem(14px);
    padding: 0 spacing();
    height: rem(46px);
    margin-bottom: spacing(loose);
    border: solid rem(2px);
    font-weight: font-weight(bold);

    @include placeholder {
      text-transform: uppercase;
      letter-spacing: 0.07142em;
      color: $black;
    }

    &:focus {
      border-color: $offgold;
    }
  }
  select {
    background-color: $white;
    text-transform: uppercase;
  }
  input[type='checkbox'] {
    margin-bottom: 0;
  }
}

.account-address__cancel {
  padding: spacing(tight) 0;
}

.account__adjacent-inputs {
  display: block;
  width: 100%;
  .account-address__phone.account__half-input input {
    margin-left: 0;
  }

  @include media-query($tablet) {
    @include display-flexbox();
  }
}

.account__half-input,
.account-address__coutnry,
.account-address__province {
  width: 100%;
  @include media-query($tablet) {
    width: 50%;

    &:last-child {
      margin-left: spacing(extra-tight); 
    }
  }
}

.account-address__province {
  @include display-flexbox();
  @include flex-direction(column);
  @include justify-content(flex-end);
}

.account-address__btn-container {
  @include display-flexbox();
  margin-bottom: spacing();
}

/*----------- Account Address Checkbox -----------*/


#account-address__checkbox {
  width: auto;
  display: block;
  position: relative;
  padding-left: spacing(loose);
  text-transform: uppercase;
  font-weight: font-weight(bold);
  input:checked ~ .checkmark {
    background-color: $white;
  }
}

#account-address__checkbox input {
  position: absolute;
  opacity: 0;
  cursor: pointer;
  height: 0;
  width: 0;
  &:focus ~ .checkmark {
    border: solid rem(2px) $offgold;
  }
  &:focus ~ label {
    color: $offgold;
  }
  &:checked ~ .checkmark:after {
    display: block;
    background-color: $offgold;
  }
}

.checkmark:after {
    content: '';
    display: inline-block;
    width: 12px;
    height: 12px;
    -moz-border-radius: 50%;
    border-radius: 50%;
    position: relative;
    background: $white;
    top: 2px;
    left: 2px;
}

.checkmark {
  position: absolute;
  top: rem(-3px);
  left: 0;
  width: rem(20px);
  height: rem(20px);
  border-radius: 50%;
  border: solid rem(2px) $black;
}
.page-not-found {
  @include display-flexbox();
  @include flex-direction(column);
  @include align-items(center);
  @include justify-content(center);
  min-height: 70vh;
  text-align: center;
  padding: 0 spacing();

  @include media-query($large-up) {
    padding: 0 spacing();
  }
}
.collection__title{
  color: $white;
  font-family: font-family(sunflower);
  font-weight: normal;
  text-align: center;
  padding: spacing()*3 spacing();
  font-size: rem(32px);

  @include media-query($large-up) {
    font-size: rem(48px);
    padding: spacing()*4 spacing()*2;
  }
}

.collection__navigation{
  margin: 0 auto;
  padding: spacing();
  @include display-flexbox();
  @include align-items(flex-start);
  @include justify-content(center);
}

.collection-navigation__link{
  @include display-flexbox();
  @include flex-direction(column);
  @include justify-content(center);
  @include align-items(center);
  margin: 0 spacing();
  font-size: rem(20px);
  font-family: font-family(avenir);
  font-weight: 900;
  color: $black;
  position: relative;

  &:focus{
    .collection-navigation__icon-container{
      transform: translate(rem(5px), rem(-5px));
    }
  }

  @include media-query($large-up) {
    &:hover{
      .collection-navigation__icon-container{
        transform: translate(rem(5px), rem(-5px));
      }
    }
  }
}

.collection-navigation__icon-background{
  background-color: $offgold;
  width: rem(60px);
  height: rem(60px);
  border: rem(2px) solid $black;
  border-radius: 50%;
  position: relative;
}

.collection-navigation__icon-container{
  position: absolute;
  top: 0;
  width: rem(60px);
  height: rem(60px);
  border: rem(2px) solid $black;
  border-radius: 50%;
  @include display-flexbox();
  @include align-items(center);
  @include justify-content(center);
  padding: rem(5px);
  background-color: $white;
  margin-bottom: rem(5px);
}

.collection-navigation__icon{
  width: 90%;
  border-radius: 50%;
  border: rem(1px) solid transparent;
}

.collection__promo{
  position: relative;
  padding: spacing();
  width: 100%;
  max-width: rem(1024px);
  margin: 0 auto;

  @include media-query($large-up) {
    padding: spacing() spacing()*2;
    width: 80%;
  }
}

.collection-promo__container{
  border: rem(2px) solid $black;
  width: 100%;
}

.collection-promo__container{
  position: relative;

  &:before{
    content: '';
    border: rem(2px) solid $black;
    background-color: $white;
    position: absolute;
    width: 100%;
    height: 100%;
    left: spacing()/-2;
    top: spacing()/2;
    z-index: -1;
    transition: all 0.2s ease-in-out;
  }

  @include media-query($medium-up) {
    @include display-flexbox();
    @include align-items(center);
  }

  &:hover{
    &:before{
      background-color: $offgold;
    }
  }

  &:focus-within{
    &:before{
      background-color: $offgold;
    }
  }

  &.focus-within{
    &:before{
      background-color: $offgold;
    }
  }
}

.collection-promo__image--mobile{
  @include media-query($medium-up) {
    display: none;
  }
}

.collection-promo__image--desktop{
  display: none;

  @include media-query($medium-up) {
    display: block;
  }
}

.collection-promo__image{
  @include media-query($medium-up) {
    width: 55%;
  }
}

.collection-promo__content{
  padding: spacing();
  
  @include media-query($medium-up) {
    width: 45%;
  }
}

.collection-promo__title{
  font-family: font-family(avenir);
  font-size: rem(20px);
  font-weight: 900;
  margin: 0;
  line-height: 1;

  @include media-query($large-up) {
    font-size: rem(20px);
  }
}

.collection-promo__subtitle{
  font-family: font-family(avenir);
  font-size: rem(18px);
  font-weight: 900;
  line-height: 1;
  margin: 0;

  @include media-query($large-up) {
