.cart__product{
  position: relative;
  width: 100%;
  margin-bottom: spacing()*1.5;

  &.is-updating{
    opacity: 0.6;
  }

  &:focus{
    &:after{
      background-color: $offgold;
    }
  }

  &:focus-within{
    &:after{
      background-color: $offgold;
    }
  }

  &.focus-within{
    &:after{
      background-color: $offgold;
    }
  }

  @include media-query($large-up) {
    &:hover{
      &:after{
        background-color: $offgold;
      }
    }
  }

  &:after{
    content: '';
    width: 100%;
    height: 100%;
    position: absolute;
    top: spacing()/2;
    left: spacing()/2 * -1;
    border: rem(2px) solid $black;
    background-color: $white;
    z-index: 0;
    transition: background-color 0.2s ease-in-out;
