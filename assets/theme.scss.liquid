  &:before{
    content: '';
    border: rem(2px) solid $black;
    background-color: $offgold;
    position: absolute;
    width: 100%;
    height: 100%;
    left: spacing()/-2;
    top: spacing()/2;
    z-index: -1;
    transition: all 0.2s ease-in-out;
  }

  &:focus:before{
    background-color: $blue;
  }

  @include media-query($large-up) {
