  font-weight: 900;
  font-size: rem(14px);
  color: $red;
  margin: 0;
}

.product-card__price{
  padding: spacing()/2;
  font-family: font-family(avenir);
  font-weight: 900;
  font-size: rem(18px);
  color: $offgold;
}
